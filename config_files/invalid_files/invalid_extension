server{
	listen 4242;
	server_name example.org www.example.org;

	root /var/www/html;

	index index.html;

	location / {
		autoindex on;
		allowed_methods GET;
		client_max_body_size 5M;
	}

	#comment

	location /img {
		root www;
		limit_except GET HEAD {
            deny all;
        }
	}
	client_max_body_size 5M;
	autoindex off;
}